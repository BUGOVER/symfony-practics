{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-zend-opcache": "*",
    "doctrine/doctrine-bundle": "^2.11.0",
    "doctrine/doctrine-migrations-bundle": "^3.3.0",
    "doctrine/orm": "^3.0.0",
    "nelmio/api-doc-bundle": "^4.0.0",
    "phpdocumentor/reflection-docblock": "^5.3.0",
    "phpstan/phpdoc-parser": "^1.0.0",
    "symfony/asset": "^6.4.0",
    "symfony/console": "^6.4.0",
    "symfony/dotenv": "6.4.0",
    "symfony/flex": "^2.0.0",
    "symfony/framework-bundle": "^6.4.0",
    "symfony/property-access": "^6.4.0",
    "symfony/property-info": "^6.4.0",
    "symfony/runtime": "^6.4.0",
    "symfony/serializer": "^6.4.0",
    "symfony/twig-bundle": "^6.4.0",
    "symfony/yaml": "^6.4.0",
    "twig/extra-bundle": "^3.0.0",
    "twig/twig": "^3.0.0"
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "^8.0",
    "doctrine/doctrine-fixtures-bundle": "^3.5",
    "friendsofphp/php-cs-fixer": "^3.49",
    "helmich/phpunit-json-assert": "^3.5",
    "phpunit/phpunit": "^10.5",
    "roave/security-advisories": "dev-latest",
    "symfony/browser-kit": "6.4.*",
    "symfony/css-selector": "6.4.*",
    "symfony/maker-bundle": "^1.54",
    "symfony/phpunit-bridge": "^7.0",
    "symfony/stopwatch": "6.4.*",
    "symfony/web-profiler-bundle": "6.4.*"
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.4.*"
    }
  }
}
